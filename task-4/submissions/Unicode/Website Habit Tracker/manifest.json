{
    "manifest_version": 2,
  
    "name": "Website Habit Tracker",
    "description": "This extension will help you to track a website.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "history","tabs", "<all_urls>"
     ]
  }